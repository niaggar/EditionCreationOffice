<Window
    x:Class="PruebaControlOpenXML.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:PruebaControlOpenXML"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <StackPanel VerticalAlignment="Center">
        <Button
            HorizontalAlignment="Center"
            Click="Button_Click"
            Content="Probar crear tabla" />
        <Button
            HorizontalAlignment="Center"
            Click="Button_Click_1"
            Content="Probar crear y guardar images" />
        <Button
            HorizontalAlignment="Center"
            Click="Button_Click_2"
            Content="Probar base64 imagenes" />
        <Button
            HorizontalAlignment="Center"
            Click="Button_Click_3"
            Content="Probar archivos imagenes" />
    </StackPanel>
</Window>
